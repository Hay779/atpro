<script>
document.addEventListener('DOMContentLoaded', function () {
    const progressBar = document.querySelector('.progress-bar-container');
    if (progressBar) {
        const lines = progressBar.querySelectorAll('.progress-line');
        const stages = progressBar.querySelectorAll('.progress-stage');
        
        lines.forEach(line => line.style.backgroundColor = ''); 
        stages.forEach(stage => stage.classList.remove('active'));
        if(stages.length > 0) stages[0].classList.add('active'); 

        if (progressBar.classList.contains('step-1')) {
        } else if (progressBar.classList.contains('step-2')) {
            if(stages.length > 1) stages[1].classList.add('active');
            if(lines.length > 0) lines[0].style.backgroundColor = 'var(--custom-accent-magenta)';
        } else if (progressBar.classList.contains('step-3')) {
            if(stages.length > 1) stages[1].classList.add('active');
            if(stages.length > 2) stages[2].classList.add('active');
            lines.forEach(line => line.style.backgroundColor = 'var(--custom-accent-magenta)');
        }
    }
});
</script>